{
    "extends": "eslint-config-standard",
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "quotes": ["error", "double"]
        ,"indent": ["error", "tab"]
        ,"no-tabs": "off"
        ,"semi": ["error", "always"]
        ,"comma-style": ["error", "first"]
        
        ,"block-scoped-var": "warn"
        ,"complexity": ["warn", 30]
        ,"consistent-return": "warn"
        ,"guard-for-in": "warn"
    }
}